.Title {
  width: 100%;
  background-color: #f70719;
  display: flex;
  height: 250rpx;
}
/* 订单 */
.orderTop {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 90rpx;
}
.myorder-text {
  font-size: 35rpx;
  color: rgb(26, 24, 24);
  margin: 40rpx;
  width: 60%;
}
.myorderlook-text {
  font-size: 30rpx;
  color: gray;
  position: relative;
  right: 20rpx;
  width: 60%;
  text-align: right;
}
.next-image {
  width: 20rpx;
  height: 25rpx;
  position: relative;
  right: 10rpx;
}
.navs {
  display: flex;
  background-color: white;
}
.nav-item {
  width: 25%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
}
.nav-item .nav-image {
  width: 60rpx;
  height: 60rpx;
  margin: 5rpx;
}
.nav-item text {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: gray;
}
.image {
  height: 120rpx;
  width: 120rpx;
  border-radius: 7rpx;
  margin: 50rpx 20rpx 50rpx 50rpx;
}

.name {
  height: 70rpx;
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 60rpx;
  color: white;
}

.Intro {
  /* width: 560rpx; */
  height: 40rpx;
  display: flex;
}
.topIn{
  height: 100rpx;
  background-color: white;
  border-radius: 10rpx;
  margin: -47rpx 3% 0rpx 3%;
  
}
.intro {
  width: 500rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  margin-left: 15rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  color: white;
}

.introImage {
  height: 40rpx;
  width: 40rpx;
}

.goLogin {
  line-height: 170rpx;
  text-align: center;
  width: 100%;
}

.Data {
  margin: 0rpx 3% 0rpx 3%;
  border-radius: 10rpx;
  padding: 0rpx 10rpx;
  background-color: white;
}
.allDing{
  margin: 0rpx 3% 0rpx 3%;
  border-radius: 10rpx;
  background-color: white;
}
.Data1 {
  margin: 0rpx 3% 0rpx 3%;
  border-radius: 10rpx;
  padding-left: 30rpx;
  background-color: white;
}

.data {
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  display: flex;
  font-size: 33rpx;
}

.dataIcon {
  height: 60rpx;
  width: 60rpx;
  margin: 15rpx 25rpx 15rpx 15rpx;
}

.dataText {
  width: 550rpx;
}

.order {
  height: 40rpx;
  margin: 25rpx 0rpx;
  color: white;
  width: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 30rpx;
  background-color: #ff2d2d;
}

.line {
  height: 1rpx;
  background-color: #f0f0f0;
  margin-left: 40rpx;
}

button::after {
  border: none;
}
